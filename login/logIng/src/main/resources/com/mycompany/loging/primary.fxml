<VBox alignment="CENTER" prefHeight="394.0" prefWidth="240.0" spacing="20.0" style="-fx-background-color: CYAN;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.loging.PrimaryController">
